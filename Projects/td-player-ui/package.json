{
  "name": "td-player-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "nx serve",
    "start:shell": "ng serve shell",
    "start:player-landing": "ng serve playerLandingPage --port 4201",
    "start:nft-marketplace": "ng serve nft-marketplace --port 4202",
    "start:remotes-then-shell": "concurrently \"npm run start:player-landing\" \"npm run start:nft-marketplace\" && timeout /t 15 && npm run start:shell",
    "start:all-remotes": "concurrently \"npm run start:player-landing\" \"npm run start:nft-marketplace\"",
    "start:remote": "nx serve playerLandingPage",
    "build": "nx build",
    "watch": "nx build --watch --configuration development",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "serve:ssr:shell": "node dist/shell/server/server.mjs",
    "serve:ssr:playerLandingPage": "node dist/playerLandingPage/server/server.mjs",
    "lint": "eslint projects --ext .ts",
    "lint:fix": "eslint projects --ext .ts --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "format": "npm run prettier:fix && npm run lint:fix",
    "pre-commit": "npm run lint && npm run prettier",
    "pre-commit:fix": "npm run lint:fix && npm run prettier:fix",
    "check-all": "npm run lint && npm run prettier && npm test",
    "fix-all": "npm run lint:fix && npm run prettier:fix",
    "prepare": "husky install",
    "serve:ssr:nft-marketplace": "node dist/nft-marketplace/server/server.mjs"
  },
  "prettier": {
    "overrides": [
      {
        "files": "Top Dog Arena",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "lint-staged": {
    "*.{ts,html}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,json,scss,css,md}": [
      "prettier --write"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.1.2",
    "@angular/common": "^20.1.0",
    "@angular/compiler": "^20.1.0",
    "@angular/core": "^20.1.0",
    "@angular/forms": "^20.1.0",
    "@angular/platform-browser": "^20.1.0",
    "@angular/platform-browser-dynamic": "^20.1.2",
    "@angular/platform-server": "^20.1.0",
    "@angular/router": "^20.1.0",
    "@angular/ssr": "^20.1.1",
    "@module-federation/enhanced": "^0.15.0",
    "express": "^5.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-architects/module-federation": "^20.0.0",
    "@angular-architects/native-federation": "^20.1.2",
    "@angular-builders/custom-webpack": "^20.0.0",
    "@angular-devkit/build-angular": "^20.1.1",
    "@angular-devkit/core": "^20.1.1",
    "@angular-devkit/schematics": "^20.1.1",
    "@angular/build": "^20.1.1",
    "@angular/cli": "^20.1.1",
    "@angular/compiler-cli": "^20.1.0",
    "@angular/elements": "^20.1.2",
    "@nx/angular": "21.2.4",
    "@nx/devkit": "21.2.4",
    "@nx/jest": "21.2.4",
    "@nx/js": "21.2.4",
    "@nx/module-federation": "21.2.4",
    "@nx/playwright": "21.2.4",
    "@nx/web": "21.2.4",
    "@nx/webpack": "21.2.4",
    "@nx/workspace": "21.2.4",
    "@playwright/test": "^1.36.0",
    "@schematics/angular": "^20.1.1",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.17.19",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "@typescript-eslint/utils": "^8.29.0",
    "angular-eslint": "20.1.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-playwright": "^1.6.2",
    "eslint-plugin-prettier": "^5.5.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-preset-angular": "~14.6.0",
    "lint-staged": "^16.1.2",
    "nx": "21.2.4",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.17",
    "ts-jest": "^29.4.0",
    "ts-node": "10.9.1",
    "typescript": "~5.8.2",
    "typescript-eslint": "8.34.1",
    "zone.js": "^0.15.1"
  }
}
